<h1>New User Signup!</h1>

<%= form_for(user, url: signup_path) do |f| %>

  <div class="field">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up!" %>
  </div>
<% end %>

<%= link_to 'Back', root_path %>
